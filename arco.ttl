cis:isRelatedToRiT rdf:type owl:ObjectProperty .


###  https://w3id.org/arco/ontology/arco/hasAlternativeDiscipline
arco:hasAlternativeDiscipline rdf:type owl:ObjectProperty ;
                              owl:inverseOf arco:isAlternativeDisciplineOf ;
                              rdfs:domain arco:CulturalProperty ;
                              rdfs:range cis:SubjectDiscipline ;
                              rdfs:comment "Questa proprietà permette di associare un bene culturale a discipline scientifiche, alternative a quella principale, cui fa riferimento."@it ,
                                           "This property relates a cultural property to a related scientific discipline, alternative to the main one."@en ;
                              rdfs:isDefinedBy <https://w3id.org/arco/ontology/arco> ;
                              rdfs:label "ha altra disciplina"@it ,
                                         "has alternative discipline"@en ;
                              owl:versionInfo "stabile"@it ,
                                              "stable"@en ;
                              a-cat:iccdNormTag "ICCD Normativa PST: CT/CTA, versione 4.00: OG/CTA" .


###  https://w3id.org/arco/ontology/arco/hasArchaeologicalMaterialCategory
arco:hasArchaeologicalMaterialCategory rdf:type owl:ObjectProperty ;
                                       rdfs:subPropertyOf core:hasCategory ;
                                       owl:inverseOf arco:isArchaeologicalMaterialCategoryOf ;
                                       rdfs:domain arco:ArchaeologicalMaterial ;
                                       rdfs:range arco:ArchaeologicalMaterialCategory ;
                                       rdfs:comment "Questa proprietà collega un materiale archeologico alla sua categoria (es.: ceramiche, industria litica, intonaco, legno, etc.)"@it ,
                                                    "This property relates the archaeological material to its category (e.g.: ceramics, plaster, lithic materials, etc.)"@en ;
                                       rdfs:isDefinedBy <https://w3id.org/arco/ontology/arco> ;
                                       rdfs:label "ha categoria di materiale archeologico"@it ,
                                                  "has archaeological material category"@en ;
                                       owl:versionInfo "stabile"@it ,
                                                       "stable"@en ;
                                       a-cat:iccdNormTag "ICCD Normativa TMA: MA/MAC/MACC" .


###  https://w3id.org/arco/ontology/arco/hasCartographicClassification
arco:hasCartographicClassification rdf:type owl:ObjectProperty ;
                                   rdfs:subPropertyOf core:hasClassification ;
                                   owl:inverseOf arco:isCartographicClassificationOf ;
                                   rdfs:domain arco:CulturalProperty ;
                                   rdfs:range arco:CartographicClassification ;
                                   rdfs:comment "Questa proprietà collega un bene culturale alla sua classificazione cartografica."@it ,
                                                "This property relates a cultural property to its cartographic classification."@en ;
                                   rdfs:isDefinedBy <https://w3id.org/arco/ontology/arco> ;
                                   rdfs:label "ha classificazione cartografica"@it ,
