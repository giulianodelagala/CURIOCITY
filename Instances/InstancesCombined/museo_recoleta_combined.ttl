<http://curiocity.org/m02_s03_212_x/Object> a ecrm:E22_Man-Made_Object,
        owl:NamedIndividual ;
    rdfs:label "Virgen de Belén"^^xsd:string ;
    ecrm:P2_has_type <http://curiocity.org/m02_s03_212_x/Utility> ;
    ecrm:P3_has_note "La Virgen Cusqueña con el Niño es una representación artística de la iconografía mariana al estilo cusqueño. En esta pintura se presenta a la Virgen María sosteniendo en brazos al Niño Jesús.  La escena refleja la actitud enternecida de la Virgen y la dulzura de la expresión del Niño en brazos de su madre.  Resalta la característica de decorados en dorado, tìpicos de la Escuela Cusqueña."^^xsd:string ;
    ecrm:P44_has_condition <http://curiocity.org/m02_s03_212_x/Current_Condition> ;
    ecrm:P45_consists_of <http://curiocity.org/Oleo_sobre_lienzo%2C_con_decorados_en_dorado%2C_Escuela_Cusque%C3%B1a> ;
