    rdfs:label "Chomba Ao 1550"^^xsd:string ;
    ecrm:P2_has_type <http://curiocity.org/m02_s01_001_p/Utility> ;
    ecrm:P3_has_note "Esta tinaja es considerada una de las ms antigua del Per, con capacidad de 800 litros.  Segn una investigacin  fue trada del Valle de Vtor y probablemente fabricada por artesanos espaoles, fundadores de Arequipa.  En la tinaje se encuentra un inscrito que seala: \"San Rafael ao de 1550\",  fecha que aducira a la creacin del objeto.   Adems se halla un grabado en madera con la tcnica de pirograbado que reza: \" De la bodega al convento truecas, chomba tu destino hoy la paz ayer el vino,  y dando contentamiento calma la sed del sediento,  a los ojos que te ven cumples tu misin tan bien no hay nadie que te resista: ayer el gusto hoy la vista te dicen Salud Amen\"  (Padre Fermn Cebrecos). "^^xsd:string ;
    ecrm:P44_has_condition <http://curiocity.org/m02_s01_001_p/Current_Condition> ;
    ecrm:P45_consists_of :Arcilla ;
    ecrm:P48_has_preferred_identifier :m02_s01_001_p ;
